{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "Get started with typescript",
  "homepage": "https://github.com/TimPietrusky/typescript-starter#readme",
  "bugs": {
    "url": "https://github.com/TimPietrusky/typescript-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimPietrusky/typescript-starter.git"
  },
  "license": "MIT",
  "author": "Tim Pietrusky",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint packages/{**/*,*}.ts",
    "ncu": "npx npm-check-updates -u --dep=prod,dev",
    "ncu:minor": "npm run ncu -- -t minor",
    "ncu:patch": "npm run ncu -- -t patch",
    "spj": "npx sort-package-json",
    "start": "npx ts-node-esm src/index.ts",
    "test": "jest",
    "toc": "npx markdown-toc README.md -i"
  },
  "dependencies": {
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@swc/core": "^1.3.68",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1"
  }
}
